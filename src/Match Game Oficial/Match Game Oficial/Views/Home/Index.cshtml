<link rel="stylesheet" href="~/css/jogos.css" asp-append-version="true" />

@{
    ViewData["Title"] = "Home Page";
}

<div id="js-preloader" class="js-preloader loaded">

    <div class="preloader-inner">

        <span class="dot"></span>
        <div class="dots">
            <span></span>
            <span></span>
            <span></span>

        </div>
    </div>
</div>


