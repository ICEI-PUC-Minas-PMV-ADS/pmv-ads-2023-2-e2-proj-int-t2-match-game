@model Match_Game_Oficial.Models.Esqsenha

@{
    ViewData["Title"] = "Inserir Código de Verificação";
}

<h1>Inserir Código de Verificação</h1>

@using (Html.BeginForm("RedefinirSenha", "EsqsenhaController", FormMethod.Post))
{
    @Html.LabelFor(model => model.Codigo)
    @Html.TextBoxFor(model => model.Codigo)
    @Html.ValidationMessageFor(model => model.Codigo)

    <input type="submit" value="Redefinir Senha" />
}
